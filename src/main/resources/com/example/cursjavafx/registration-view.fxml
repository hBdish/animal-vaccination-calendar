<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" prefHeight="335.0" prefWidth="379.0" spacing="20.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.cursjavafx.RegistrationController">
   <TextField fx:id="name" prefHeight="26.0" prefWidth="544.0">
      <opaqueInsets>
         <Insets />
      </opaqueInsets>
      <VBox.margin>
         <Insets left="12.0" right="12.0" />
      </VBox.margin></TextField>
   <TextField fx:id="login">
      <opaqueInsets>
         <Insets />
      </opaqueInsets>
      <VBox.margin>
         <Insets left="12.0" right="12.0" />
      </VBox.margin></TextField>
   <TextField fx:id="password">
      <opaqueInsets>
         <Insets />
      </opaqueInsets>
      <VBox.margin>
         <Insets left="12.0" right="12.0" />
      </VBox.margin></TextField>
    <Button onAction="#onMainButtonClick" styleClass="btn, btn-primary" text="Зарегистрироваться" />
   <Button onAction="#switchToLoginPage" text="Войти" />
</VBox>
